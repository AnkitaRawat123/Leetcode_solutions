Plus One
class Solution {
    public int[] plusOne(int[] digits) {
        int l=digits.length;
        for(int i=l-1;i>=0;i--)
            {
                if(digits[i]!=9){
                    digits[i]=digits[i]+1;
                    return digits;
                }
                else{
                    digits[i]=0;
                }
            }
        int [] a=new int[digits.length+1];
        a[0]=1;
        return a;
    }
}
